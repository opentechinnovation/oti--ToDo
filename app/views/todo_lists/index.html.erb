

    <div class="jumbotron">
      <h1>Projects Task Application</h1>
        <p>
            Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod
            tempor incididunt ut labore et dolore magna aliqua.</p>
        <p>
            <a href="" class="btn btn-primary btn-lg" role="button">Learn more</a>
        </p>
    </div>
    <div class="page-header">
        <h1>Projects <i class="glyphicon glyphicon-hdd"></i></h1>
    </div>
    <% @todo_lists.each do |todo_list| %>
    <div class="todo_list" id="<%= dom_id(todo_list) %>">
      <h2><%= todo_list.title %></h2>
      <p><%= todo_list.description %></p>
          <div class="btn-group">
            <button type="button" class="btn btn-default">
                <%= link_to "List Items", todo_list_todo_items_path(todo_list) %>
            </button>
            <button type="button" class="btn btn-default">
                <%= link_to 'Show', todo_list %></button>
            <button type="button" class="btn btn-default">
                <%= link_to 'Edit', edit_todo_list_path(todo_list) %>
            </button>
            <button type="button" class="btn btn-default">
                <%= link_to 'Destroy', todo_list, method: :delete, data: { confirm: 'Are you sure?' } %>
            </button>
          </div>
      <br class="clear" />
    </div>
<% end %>
<br>
<button type="button" class="btn btn-success btn-lg">
    <%= link_to 'New Project', new_todo_list_path %>
</button>


